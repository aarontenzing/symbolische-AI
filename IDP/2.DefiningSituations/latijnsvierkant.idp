vocabulary LatinSquareVoc {
  type cell
  type number := {1..3}
  rows: cel*cel -> Bool
  cols: cel*cel -> Bool
  value: cell -> number
}
structure Board:LatinSquareVoc {
    cell := {C1,C2,C3,C4,C5,C6,C7,C8,C9}
    rows := {(C1,C2),(C2,C3),(C4,C5),(C5,C6),(C7,C8),(C8,C9)}
    cols := {(C1,C4),(C4,C7),(C2,C5),(C5,C8),(C3,C6),(C6,C9)}
    value := {(c1->1),(c3->2),(c5->1),(c7->3),(c9->1)}
}

