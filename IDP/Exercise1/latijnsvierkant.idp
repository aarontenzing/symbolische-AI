vocabulary LatinSquareVoc {
  type cel
  type columns
  type rows
  value: cel
  mapCelCol : 
  position: cel * cols * rows -> Bool 
}

structure game:LatinSquareVoc {
    cel := {C1, C2, C3, C4, C5, C6, C7, C8, C9}
    columns := {0..2}
    rows := {0..2}
    value := {(c1,1), (c2,0), (c3,2), (c4,0), (c5,1), (c6,0), (c7, 3), (c8, 0), (c9, 1)}
    position := {(C0,0,0->True),(C1,0,1),(C2,0,2), (C3,1,0), (C4,1,1), (C5,1,1), (C6,1,2), (C7,2,0), (C8,2,1), (C9,2,2)}
}

